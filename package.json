    "@web3modal/wagmi": "^5.1.11",
    "@xmtp/node-sdk": "^2.1.0",
    "@xmtp/xmtp-js": "^13.0.4",
    "alchemy-sdk": "^3.6.0",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
