{
  "name": "rainmaker-arena",
  "version": "1.0.0",
  "private": true,
  "description": "Interactive crypto game show platform combining Rainmaker community rewards, price prediction games, and Telegram/Web experience",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:env": "node test-env.js",
    "bot": "node scripts/telegram-bot.js",
    "bot:start": "npx pm2 start ecosystem.config.js",
    "bot:stop": "npx pm2 stop rainmaker-telegram-bot",
    "bot:restart": "npx pm2 restart rainmaker-telegram-bot",
    "bot:status": "npx pm2 status",
    "bot:logs": "npx pm2 logs rainmaker-telegram-bot",
    "bot:permanent": "npx pm2 start ecosystem.config.js && npx pm2 save && npx pm2 startup",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "verify-deployment": "node verify-deployment.js"
  },
  "dependencies": {
    "@chainlink/contracts": "^1.2.0",
    "@coinbase/agentkit": "^0.0.0-nightly-20250501210425",
